<aside class="aside-back">    
    <% if (locals.errors) { %>
        <h2>ERRORES DEL LADO DEL BACKEND</h2>
        <% errors.forEach((error) => {%>
           
            <div class="alert alert-warning" role="alert">
                <i class="fa-solid fa-triangle-exclamation"></i>&nbsp; &nbsp;<%= error.msg %>
            </div>
            <% }); %>
    <% } %>
</aside>